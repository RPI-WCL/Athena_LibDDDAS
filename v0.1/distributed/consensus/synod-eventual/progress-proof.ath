 	define CND-def :=
	(forall T i p b Q .
		(
					  (prp p) 
					& (P1+P2-True b)
					& (nonfaulty p) 
					& ((all-inQ-NF Q) & (rdy2snd-1a-all-inQ p Q (rho T i) b))

		)

		==> 

		(
					  (prp p) 
					& (P1+P2-True b)
					& (nonfaulty p) 
					& (forall a . (inQ a Q) 
						                ==> 
						                      (nonfaulty a) 
						                    & (ready-to (rho T i) p (send p (consM p a 1a b nilV)))
					  )
		)

	)

